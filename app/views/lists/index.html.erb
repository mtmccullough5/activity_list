<h1>Lists</h1>
<%= link_to 'New List', new_list_path %>
<% @lists.each do |list|  %>
    <h2><%= list.name %></h2>
    <%= link_to 'Show', list_path(list) %>
    <%= link_to 'Edit', edit_list_path(list) %>
    <%= link_to 'Destroy', list_path(list),
        method: :delete,
        data: {confirm: 'Are you sure?'} %>
<% end %>
<p>Find me in app/views/lists/index.html.erb</p>
